{
  "dependencies": {
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "frontend": "cd usec-blog-frontend && npm run dev",
    "backend": "cd usec-blog-backend && npm start",
    "start": "concurrently --names \"FRONTEND,BACKEND\" --prefix-colors \"bgBlue,bgRed\" --prefix \"{time} - [{name}]\" \"npm run backend\" \"npm run frontend\""
  }
}
